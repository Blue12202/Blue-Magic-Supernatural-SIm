--Functions

 local Functions = Window:NewTab("Functions")
 local FunctionsSection = Functions:NewSection("Functions")
FunctionsSection:NewLabel("Don't use these features too frequently,")
FunctionsSection:NewLabel("or you might lag a lot")
-------------
FunctionsSection:NewButton("AutoFarm", "AutoFarms, aslong as LVLUP equipped", function()
while true do
local A_1 = 
{
	[1] = "GainMuscle"
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)
wait(0.1)
end
end)
-------------
FunctionsSection:NewButton("AutoSell", "AutoSells, every 2 seconds", function()
while true do
local A_1 = 
{
	[1] = "SellMuscle"
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)
wait(2)
 end
end)
-------------
FunctionsSection:NewButton("KillAura", "Gives you a KillAura", function()
while true do
local A_1 = 
{
	[1] = "AttackClick"
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)

wait(0.1)
 end
end)

--Visuals
local Visuals = Window:NewTab("Visuals")
local VisualsSection = Visuals:NewSection("Visuals")
VisualsSection:NewLabel("Conceal required!")
-------------
VisualsSection:NewSlider("NegativeLevel", "Change your level to negative", 0, -6666, function(s) -- -6666 (MaxValue) | 0 (MinValue)
local A_1 = 
{
	[1] = "UpdateSettingRevealLevel", 
	[2] = (s)
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)
end)
-------------
VisualsSection:NewSlider("NegativeLevel", "Change your level to negative", -6666, -99999, function(s) -- -99999 (MaxValue) | -6666 (MinValue)
local A_1 = 
{
	[1] = "UpdateSettingRevealLevel", 
	[2] = (s)
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)
end)
-------------
VisualsSection:NewSlider("NegativeLevel", "Change your level to negative", -99999, -666666, function(s) -- -666666 (MaxValue) | -99999 (MinValue)
local A_1 = 
{
	[1] = "UpdateSettingRevealLevel", 
	[2] = (s)
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)
end)
-------------
VisualsSection:NewSlider("NegativeLevel", "Change your level to negative", -999999999999999999999, -6666666666666666666, function(s) -- -999999999999999999999 (MaxValue) | -6666666666666666666 (MinValue)
local A_1 = 
{
	[1] = "UpdateSettingRevealLevel", 
	[2] = (s)
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)
end)
-------------
VisualsSection:NewLabel("Read before use !")
VisualsSection:NewLabel("The names are changing every 2 seconds,")
VisualsSection:NewLabel("to prevent lag and ensure a smooth visual,")
VisualsSection:NewLabel("try to use the features in a 2 second delay,")
VisualsSection:NewLabel("also don't use it too often,")
VisualsSection:NewLabel("or it will glitch out!")
-------------
VisualsSection:NewTextBox("Name1", "Choose your first name", function(txt)
wait(2)
while true do
local A_1 = 
{
	[1] = "ChangeRoleplayName", 
	[2] = (txt)
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)

wait(2)
 end	
end)
-------------
VisualsSection:NewTextBox("Name2", "Choose your second name", function(txt)
while true do
local A_1 = 
{
	[1] = "ChangeRoleplayName", 
	[2] = (txt)
}
local Event = game:GetService("ReplicatedStorage").RemoteEvent
Event:FireServer(A_1)

wait(2)
 end	
end)
